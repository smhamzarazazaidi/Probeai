{
    "rewrites": [
        {
            "source": "/api/health",
            "destination": "/api/health.js"
        },
        {
            "source": "/api/surveys",
            "destination": "/api/surveys.js"
        },
        {
            "source": "/api/notifications",
            "destination": "/api/notifications.js"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}